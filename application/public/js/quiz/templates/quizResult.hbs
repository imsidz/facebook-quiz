<div class="row">
    <div class="col-md-12">
        {{#if isScoreBased}}
            {{#if showResultScore}}
                <div class="result-graph" data-dimension="200" data-text="35%" data-width="30" data-fontsize="30" data-percent="{{percentScore}}" data-fgcolor="#19E27F" data-bgcolor="#fff" data-textcolor="#555" data-startpercent="0" data-animationstep="1"></div>
            {{/if}}
        {{/if}}
        <div class="quiz-result-container">
            <div class="quiz-result">
                <h1 class="text-center quiz-result-title">{{title}}</h1>
                {{#if image}}
                    <img class="quiz-result-image" src="{{resultImage}}" alt="">
                {{/if}}
                {{#if description}}
                    <div class="quiz-result-description">{{{description}}}</div>
                {{/if}}
            </div>

            {{#if isScoreBased}}
                {{#if showCorrectAnswersAtTheEnd }}
                    <div style="margin: 20px 0px;"><div class="view-correct-ans-btn btn btn-green"><span>{{__ "seeCorrectAnswers"}}</span></div></div>
                {{/if}}
            {{/if}}
            <div class="correctAnswers"></div>
            <div class="result-share-prompt">
                <!--<img src="/images/share-on-fb-prompt.jpg">
                <br><br>-->
                <br/>
                <h2 class="text-center">{{__ "heyShareItPrompt"}}</h2>
                <div class="result-share-buttons-container" data-url="{{shareResultLink}}" data-text="{{__ "iGot"}} '{{title}}'.. {{quizTopic}}" data-image="{{contentUrl image}}">

                </div>
            </div>
            <br><br><br>
        </div>
        <a href="{{quizRetryUrl}}"><strong>{{__ "tryThisQuizAgain"}}</strong></a>
        {{#if showPageLikePrompt}}
            {{#with socialMedia}}
                {{#with facebook}}
                    <div class="panel panel-default" style="margin-top: 30px; margin-bottom: 0px;">
                        <div class="panel-body">
                            <div class="row text-center">
                                <div class="col-md-7 col-xs-12 col-sm-12">
                                    <h4 class="" style="margin-bottom: 0px;"><p>{{__ "wannaTryMoreQuizzes"}}</p><strong>{{__ "likeUsOnFB"}}</strong></h4>
                                </div>
                                <div class="col-md-5 col-xs-12 col-sm-12">
                                    <iframe src="//www.facebook.com/plugins/likebox.php?href={{pageUrl}}&amp;width&amp;height=258&amp;colorscheme=light&amp;show_faces=false&amp;header=false&amp;stream=false&amp;show_border=false" scrolling="no" frameborder="0" style="border:none; overflow:hidden; height:60px; width: 100%;" allowtransparency="true"></iframe>
                                </div>
                            </div>
                        </div>
                    </div>
                {{/with}}
            {{/with}}
        {{/if}}
    </div>
    <div id="resultSharePromptModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span><span class="sr-only">{{__ "closeBtn"}}</span></button>
                    <h4 class="modal-title">{{__ "heyShareItPrompt"}}</h4>
                </div>
                <div class="modal-body">
                    <br>
                    <h4 class="text-center"><strong>{{title}}</strong></h4>
                    {{#if description}}
                        <p style="padding: 0px 30px;" class="text-center">{{{description}}}</p>
                    {{/if}}
                    <br>
                    <div class="text-center">
                        <a data-url="{{shareResultLink}}" href="javascript:;" class="share-on-fb btn btn-fb btn-lg share-result-btn"><span><i class="fa fa-2x fa-facebook"></i>{{__ "shareOnFB"}}</span></a>
                    </div>
                    <br>
                    <p class="text-muted text-center">
                        <span data-dismiss="modal" style="cursor: pointer;">{{__ "closeBtn"}}</span>
                    </p>
                    <br>
                    <h4><strong>{{__ "likeUsOnFB"}}</strong></h4>
                    <div class="text-center">
                        {{#with socialMedia}}
                            {{#with facebook}}
                                <div class="fb-like fb-like-iframe" data-href="{{pageUrl}}" data-layout="button_count" data-action="like" data-show-faces="false" data-share="false"></div>
                            {{/with}}
                        {{/with}}
                    </div>
                    <br><br>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
</div>


